{
	"name": "template-repos-ts",
	"version": "1.0.0",
	"description": "Template repository for using TypeScript",
	"main": "index.js",
	"scripts": {
		"start": "parcel public/index.html",
		"build": "parcel build public/index.html",
		"lint": "yarn lint:eslint && yarn lint:format",
		"lint:eslint": "eslint . --ext .ts,.js --fix",
		"lint:format": "prettier --write '**/*.{js,json,md,yml,html}'"
	},
	"author": "",
	"license": "MPL-2.0",
	"dependencies": {
		"graphiql": "^0.17.5",
		"graphiql-explorer": "^0.6.2",
		"graphql": "14.1.1",
		"react": "^16.13.1",
		"react-dom": "^16.13.1"
	},
	"devDependencies": {
		"@types/react": "^16.9.35",
		"@types/react-dom": "^16.9.8",
		"@typescript-eslint/eslint-plugin": "^2.34.0",
		"@typescript-eslint/parser": "^2.34.0",
		"eslint": "^7.0.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-config-xo": "^0.29.1",
		"eslint-config-xo-typescript": "^0.29.0",
		"eslint-plugin-prettier": "^3.1.3",
		"husky": "4.2.5",
		"parcel": "^1.12.4",
		"prettier": "2.0.5",
		"typescript": "^3.9.2"
	},
	"husky": {
		"hooks": {
			"pre-commit": "yarn lint"
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/dev-protocol/template-repos-ts.git"
	},
	"bugs": {
		"url": "https://github.com/dev-protocol/template-repos-ts/issues"
	},
	"homepage": "https://github.com/dev-protocol/template-repos-ts#readme"
}
