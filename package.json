{
	"name": "template-repos-ts",
	"version": "1.0.0",
	"description": "Template repository for using TypeScript",
	"main": "index.js",
	"scripts": {
		"start": "parcel public/index.html",
		"build": "parcel build public/index.html",
		"lint": "yarn lint:eslint && yarn lint:format",
		"lint:eslint": "eslint . --ext .ts,.tsx,.js --fix",
		"lint:format": "prettier --write '**/*.{js,json,md,yml,html}'"
	},
	"author": "",
	"license": "MPL-2.0",
	"dependencies": {
		"graphiql": "^1.0.0",
		"graphiql-explorer": "^0.9.0",
		"graphql": "16.4.0",
		"react": "^18.0.0",
		"react-dom": "^18.0.0"
	},
	"devDependencies": {
		"@types/react": "18.0.9",
		"@types/react-dom": "18.0.3",
		"@typescript-eslint/eslint-plugin": "5.22.0",
		"@typescript-eslint/parser": "5.22.0",
		"eslint": "8.14.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-config-xo": "0.40.0",
		"eslint-config-xo-typescript": "0.50.0",
		"husky": "7.0.4",
		"parcel": "1.12.4",
		"prettier": "2.6.2",
		"typescript": "4.6.4"
	},
	"browserslist": "since 2017-06",
	"husky": {
		"hooks": {
			"pre-commit": "yarn lint"
		}
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/dev-protocol/template-repos-ts.git"
	},
	"bugs": {
		"url": "https://github.com/dev-protocol/template-repos-ts/issues"
	},
	"homepage": "https://github.com/dev-protocol/template-repos-ts#readme"
}
